<Window x:Class="Manage_Competitions.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        mc:Ignorable="d"
        Title="MainWindow" Height="500" Width="800" Visibility="Visible" WindowStyle="None"
        AllowsTransparency="True" Background="Transparent"
        xmlns:nav="clr-namespace:Manage_Competitions.State.Navigator"
        xmlns:pages="clr-namespace:Manage_Competitions.Views">
    <Grid>
        <Frame 
            Content="{Binding Navigator.CurrentViewModel}"
            NavigationUIVisibility="Hidden"/>
    </Grid>
</Window>
